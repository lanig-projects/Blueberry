{
  "compilerOptions": {
    "target": "es2022",
    "module": "esnext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@assets/*": [
        "./src/assets/*"
      ],
      "@config/*": [
        "./src/config/*"
      ],
      "@environment/*": [
        "./src/environment/*"
      ],
      "@routes/*": [
        "./src/routes/*"
      ],
      "@shared/*": [
        "./src/app/@shared/*"
      ],
      "@core/*": [
        "./src/app/@core/*"
      ],
      "@default/*": [
        "./src/app/@default/*"
      ]
    }
  },
  "include": [
    "src"
  ]
}
